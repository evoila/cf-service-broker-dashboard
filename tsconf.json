{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": false,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "target": "ES2015",
    "lib": [
      "es2016",
      "ES2015",
      "dom"
    ],
    "module": "commonjs",
    "paths": {
      "custom-module-type": ["types/custom-module-type"]
    }
  }
}